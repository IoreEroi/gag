-- CONFIG
local KEYS_URL = "https://raw.githubusercontent.com/IoreEroi/gag/main/keys.txt"

-- hash della key che TU hai generato per questo cliente
local CLIENT_HASH = "3e33252c5ebd41242f306323ea377743f86dbcd3399aa873032cc299498ac524"

-- scarica lista hash
local success, data = pcall(function()
    return game:HttpGet(KEYS_URL)
end)

if not success then
    warn("Errore nel controllo chiavi")
    return
end

-- controlla se l'hash esiste
if not data:find(CLIENT_HASH, 1, true) then
    warn("Key not valid or revoked")
    return
end
local tp = game:GetService("TeleportService")
local plrs = game:GetService("Players")
local cg = game:GetService("CoreGui")
local uis = game:GetService("UserInputService")

local gui = Instance.new("ScreenGui", cg)
gui.Name = "Joiner"

local f = Instance.new("Frame", gui)
f.Size = UDim2.new(0,200,0,130)
f.Position = UDim2.new(0.5,-100,0.5,-65)
f.BackgroundColor3 = Color3.new(0,0,0)
f.BackgroundTransparency = 0.5
f.BorderSizePixel = 0
f.Active = true
f.Draggable = true
Instance.new("UICorner", f).CornerRadius = UDim.new(0,8)

local t = Instance.new("TextLabel", f)
t.Size = UDim2.new(1,0,0,30)
t.BackgroundTransparency = 1
t.Text = "Joiner"
t.Font = Enum.Font.GothamBold
t.TextSize = 16
t.TextColor3 = Color3.new(1,1,1)

local b = Instance.new("TextBox", f)
b.Size = UDim2.new(1,-20,0,28)
b.Position = UDim2.new(0,10,0,40)
b.PlaceholderText = "Job ID"
b.ClearTextOnFocus = false
b.Text = ""
b.Font = Enum.Font.Gotham
b.TextSize = 14
b.TextColor3 = Color3.new(1,1,1)
b.BackgroundColor3 = Color3.new(1,1,1)
b.BackgroundTransparency = 0.9
b.BorderSizePixel = 0
Instance.new("UICorner", b).CornerRadius = UDim.new(0,6)

local j = Instance.new("TextButton", f)
j.Size = UDim2.new(1,-20,0,28)
j.Position = UDim2.new(0,10,0,75)
j.Text = "Join"
j.Font = Enum.Font.GothamBold
j.TextSize = 14
j.TextColor3 = Color3.new(1,1,1)
j.BackgroundColor3 = Color3.new(1,1,1)
j.BackgroundTransparency = 0.85
j.BorderSizePixel = 0
Instance.new("UICorner", j).CornerRadius = UDim.new(0,6)

local tg = Instance.new("TextButton", f)
tg.Size = UDim2.new(1,-20,0,18)
tg.Position = UDim2.new(0,10,0,108)
tg.Text = "Spam: OFF (L)"
tg.Font = Enum.Font.Gotham
tg.TextSize = 12
tg.TextColor3 = Color3.new(1,1,1)
tg.BackgroundColor3 = Color3.new(1,1,1)
tg.BackgroundTransparency = 0.9
tg.BorderSizePixel = 0
Instance.new("UICorner", tg).CornerRadius = UDim.new(0,4)

local spam = false

local function flip()
	spam = not spam
	tg.Text = spam and "Spam: ON (L)" or "Spam: OFF (L)"
	tg.BackgroundTransparency = spam and 0.7 or 0.9
end

tg.MouseButton1Click:Connect(flip)

uis.InputBegan:Connect(function(i,gp)
	if gp then return end
	if i.KeyCode == Enum.KeyCode.L then
		flip()
	end
end)

j.MouseButton1Click:Connect(function()
	local id = b.Text
	if id == "" then
		j.Text = "No ID"
		task.wait(1)
		j.Text = "Join"
		return
	end

	if spam then
		task.spawn(function()
			while spam do
				pcall(tp.TeleportToPlaceInstance, tp, game.PlaceId, id, plrs.LocalPlayer)
				task.wait(0.1)
			end
		end)
	else
		tp:TeleportToPlaceInstance(game.PlaceId, id, plrs.LocalPlayer)
	end
end)
